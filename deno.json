{
  "name": "@sobanieca/jsonr",
  "description": "CLI tool for interacting with json http api's",
  "version": "4.0.0",
  "license": "MIT",
  "exports": "./main.js",
  "compilerOptions": {
    "checkJs": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "useUnknownInCatchVariables": false
  },
  "fmt": {
    "include": [".", "test/"]
  },
  "lint": {
    "include": [".", "test/"],
    "rules": {
      "exclude": ["no-import-prefix"]
    }
  },
  "tasks": {
    "run": "deno run --unsafely-ignore-certificate-errors --allow-all main.js",
    "check": "deno fmt --check && deno lint && deno check main.js && deno check test/test.js && cd test && deno task test"
  }
}
