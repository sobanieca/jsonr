{
  "name": "@sobanieca/jsonr",
  "description": "CLI tool for interacting with json http api's",
  "version": "3.0.0",
  "license": "MIT",
  "exports": "./main.js",
  "compilerOptions": {
    "checkJs": true,
    "noImplicitAny": false,
    "strictNullChecks": false
  },
  "lint": {
    "rules": {
      "exclude": ["no-import-prefix"]
    }
  },
  "tasks": {
    "run": "deno run --unsafely-ignore-certificate-errors --allow-all main.js"
  }
}
